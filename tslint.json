{
  "extends": [
    "tslint:latest",
    "tslint-react"
  ],
  "rules": {
    "one-line": [
      false
    ],
    "indent": [
      true,
      "spaces"
    ],
    "only-arrow-functions": [
      false
    ],
    "trailing-comma": [
      true,
      {
        "multiline": "ignore",
        "singleline": "ignore"
      }
    ],
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "no-var-requires": false,
    "ordered-imports": false,
    "member-ordering": [
      false
    ],
    "object-literal-sort-keys": false,
    "object-literal-shorthand": false,
    "no-shadowed-variable": false,
    "arrow-parens": false,
    "no-console": [
      false
    ],
    "jsx-alignment": [
      false
    ]
  }
}
